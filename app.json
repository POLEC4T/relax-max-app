{
  "expo": {
    "name": "RelaxMax",
    "slug": "RelaxMax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-bg_500.png",
    "scheme": "relaxmax",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.polecat.relaxmax",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-no-bg_500.png",
        "backgroundColor": "#3B5998"
      },
      "package": "com.polecat.relaxmax"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#3B5998",
          "image": "./assets/icon-no-bg_500.png",
          "dark": {
            "image": "./assets/icon-no-bg_500.png",
            "backgroundColor": "#3B5998"
          },
          "imageWidth": 100
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "94c93b4c-a879-4463-89ad-309d1e3b45bf"
      }
    },
    "owner": "evilryu117"
  },
  "expo-router": {
    "ignoreRoute": [
      "utils/**/*",
      "types/**/*",
      "components/**/*"
    ]
  }
}
